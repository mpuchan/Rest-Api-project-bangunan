<% stuff = typeof stuff !== 'object' ? {} : stuff %>
<!-- Left side column. contains the logo and sidebar -->
<aside class="main-sidebar">

  <!-- sidebar: style can be found in sidebar.less -->
  <section class="sidebar">

    <!-- Sidebar user panel (optional) -->
    <div class="user-panel">
      <div class="pull-left image">
        <% if (user.image != null) { %>
        <img src="/images/adminprofile/<%= user.image %>" height="18" width="18" class="img-circle" alt="User Image">
        <% }else{ %>
        <img src="/images/adminprofile/usernull.png" height="18" width="18" class="img-circle" alt="User Image">
        <% } %>
      </div>
      <div class="pull-left info">
        <p><% if (user.name != null) { %>
          <%= user.name %>
          <% }else{ %>
          <%= "Admin" %>
          <% } %></p>
        <!-- Status -->
        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
      </div>
    </div>


    <!-- Sidebar Menu -->
    <ul class="sidebar-menu" data-widget="tree">
      <!-- Optionally, you can add icons to the links -->
      <li class="<% if ( stuff.url == "/admin" ) { %> active <% } %>">
        <a href="/admin"> <i class="fa fa-dashboard"></i> <span>Dashboard</span></a>
      </li>
      <li class="treeview active">
        <a href="#">
          <i class="fa fa-user-circle"></i>
          <span>Management User</span>
          <span class="pull-right-container">
            <i class="fa fa-angle-left pull-right"></i>
          </span>
        </a>
        <ul class="treeview-menu">
          <% if (user.role == 1) { %>
          <!-- <li class="<% if ( stuff.url == "/admin/superuser" ) { %> active <% } %>">
            <a href="/admin/superuser"><i class="fa fa-user"></i> <span>Pembantu Admin</span></a>
          </li> -->
          <% }else if (user.role == 2) { %>
          <% }else{ %>
          <!-- <li class="<% if ( stuff.url == "/admin/superuser" ) { %> active <% } %>">
            <a href="/admin/superuser"><i class="fa fa-user"></i> <span>Pembantu Admin</span></a>
          </li> -->
          <% } %>
          <li class="<% if ( stuff.url == "/admin/user" ) { %> active <% } %>">
            <a href="/admin/user"><i class="fa fa-users"></i> <span>User</span></a>
          </li>
        </ul>
      <li class="treeview active">
        <a href="#">
          <i class="ion-pound"></i>
          <span>Keterangan</span>
          <span class="pull-right-container">
            <i class="fa fa-angle-left pull-right"></i>
          </span>
        </a>
        <ul class="treeview-menu">
          <li class="<% if ( stuff.url == "/admin/satuan" ) { %> active <% } %>">
            <a href="/admin/satuan"><i class="fa fa-picture-o"></i> <span>Satuan Barang</span></a>
          </li>
          <li class="<% if ( stuff.url == "/admin/jenis" ) { %> active <% } %>">
            <a href="/admin/jenis"><i class="fa fa-picture-o"></i> <span>Jenis Barang</span></a>
          </li>
      </li>
    </ul>
    <li class="treeview active">
      <a href="#">
        <i class="fa fa-home"></i>
        <span>Material</span>
        <span class="pull-right-container">
          <i class="fa fa-angle-left pull-right"></i>
        </span>
      </a>
      <ul class="treeview-menu">

        <li class="<% if ( stuff.url == "/admin/material" ) { %> active <% } %>">
          <a href="/admin/material"><i class="ion ion-settings"></i> <span>Material</span></a>
        </li>
      </ul>
    </li>
    </ul>
    <!-- /.sidebar-menu -->
  </section>
  <!-- /.sidebar -->
</aside>